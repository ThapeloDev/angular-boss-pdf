{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./app",
    "outDir": "./dist/out-tsc",
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "inlineSources": true,
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "declaration": true,
    "skipLibCheck": false,
    "stripInternal": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "typeRoots": [
      "node_modules/@types/node",
      "node_modules/@types/pdfmake"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "genDir": "../temp/factories",
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "entryModule": "app/pdfmake/pdfmake.module#PdfmakeModule"
  }
}
